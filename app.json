{
  "expo": {
    "name": "ThinkTrail",
    "slug": "thinktrail",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./src/Assets/appicon/newappicon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "backgroundColor": "#2B8EDC",
      "image": "./src/Assets/appicon/splashscreen.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eladev.thinktrail",
      "buildNumber": "1.0.9",
      "splash": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f029e55a-a203-4068-b610-73c379e6291e"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "thinktrail",
            "project": "ThinkTrail"
          }
        }
      ]
    },
    "plugins": [
      "sentry-expo"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f029e55a-a203-4068-b610-73c379e6291e"
    }
  }
}
